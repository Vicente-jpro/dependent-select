<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<%= form_with url: country_provinces_path, method: :get, local: true, html: { id: "country-form" } do |form| %>
  <%= form.label :country_id, "Select Country" %>
  <%= form.collection_select :country_id, Country.all, :id, :name_country, { prompt: "Choose a country" }, { onchange: "this.form.submit();" } %>
<% end %>
